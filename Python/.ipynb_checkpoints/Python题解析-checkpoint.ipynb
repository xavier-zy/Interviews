   "source": [
    "## 请用python编写函数find_string，从文本中搜索并打印内容，要求支持通配符星号和问号。"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "def find_string(string, pat):\n",
    "    import re\n",
    "    print(re.findall(pat, string, re.I))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "['wor']\n",
      "['hello worl']\n",
      "['o w', 'orl']\n",
      "['o', 'o']\n"
     ]
    }
   ],
   "source": [
    "find_string('hello world','wor')\n",
    "find_string('hello world','.*l')\n",
    "find_string('hello world','o..')\n",
    "find_string('hello world','o')"
   ]
  },
